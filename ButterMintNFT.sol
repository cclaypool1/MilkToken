    function claimToken(string memory uri) public payable
    {
        if(msg.sender != _owner)
        {
            require(msg.value >= mintPrice, "Insuffienct BNB value");
        }
        
        uint256 paidBNB = msg.value;
        uint256 quarter = paidBNB.div(4);
        
        uint256 milkQuarter = quarter;
        uint256 butterQuarter = quarter;
        uint256 milkLPQuarter = quarter;
        uint256 butterLPQuarter = paidBNB.sub(milkQuarter).sub(butterQuarter).sub(milkLPQuarter);
        
        if(msg.sender != _owner)
        {
            //swap and create LPs
            
            //Butter first
            //swap BNB for butter
            uint256 collectedButter = butter.balanceOf(address(this));
        
             // make the swap
            address[] memory path = new address[](2);
            path[0] = uniswapV2Router.WETH();
            path[1] = butterAddress;

            uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:butterQuarter}(
            0, // accept any amount of Butter
            path,
            address(this),
            block.timestamp
            );
            
            collectedButter = butter.balanceOf(address(this));
            
            //Then Milk
            //swap BNB for Milk
            uint256 collectedMilk = milk.balanceOf(address(this));
        
             // make the swap
            path = new address[](2);
            path[0] = uniswapV2Router.WETH();
            path[1] = milkAddress;

            uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:milkQuarter}(
            0, // accept any amount of Butter
            path,
            address(this),
            block.timestamp
            );
            
            collectedMilk = milk.balanceOf(address(this));
            
            //now add the liquidity for Milk and Butter and burn
            milk.approve(address(uniswapV2Router), collectedMilk);

            // add the liquidity
            uniswapV2Router.addLiquidityETH{value: milkLPQuarter}(
            address(this),
            collectedMilk,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            burnAddress,
            block.timestamp
            );
            
            //now add the liquidity for Milk and Butter and burn
            butter.approve(address(uniswapV2Router), collectedButter);

            // add the liquidity
            uniswapV2Router.addLiquidityETH{value: butterLPQuarter}(
            address(this),
            collectedButter,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            burnAddress,
            block.timestamp
            );
        }
        
        _safeMint(msg.sender, id);
        URIs[id] = uri;
        creator[id] = msg.sender;
        
        //increment ID
        id = id + 1;
    }
    
    function collectStrayBNB() public onlyExpense
    {
        expense().transfer(address(this).balance);
